     "output_type": "display_data"
    }
   ],
   "source": [
    "# Sample data\n",
    "data = {\n",
    "    'Team': ['Team A', 'Team B', 'Team C', 'Team A', 'Team B'],\n",
    "    'Season': ['2018', '2019', '2020', '2021', '2022'],\n",
    "    'Playoff': [1, 0, 1, 0, 1],\n",
    "    'QB_P': [15, 20, 25, 30, 22],  # Example QB pay percentage\n",
    "    'Team_Salary_Cap': [200, 210, 220, 230, 240]  # Example team salary cap\n",
    "}\n",
    "\n",
    "# Create a DataFrame\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Combine QB Pay Percentage and Team Salary Cap into one value for the bar height\n",
    "df['Combined'] = df['QB_P'] * df['Team_Salary_Cap'] / 100  # Example: QB Pay as a % of Salary Cap\n",
    "\n",
    "# Pivot the DataFrame to prepare for the stacked bar chart\n",
    "pivot_df = df.pivot_table(index='Season', columns='Playoff', values='Combined', aggfunc='sum', fill_value=0)\n",
    "\n",
    "# Create a stacked bar chart\n",
    "plt.figure(figsize=(10, 6))\n",
    "pivot_df.plot(kind='bar', stacked=True, color=['red', 'blue'], edgecolor='black')\n",
    "\n",
    "# Adding titles and labels\n",
    "plt.title('Quarterback Pay Percentage vs. Seasons (Stacked by Playoff Appearance)')\n",
    "plt.xlabel('Seasons')\n",
    "plt.ylabel('QB Pay as % of Team Salary Cap')\n",
    "\n",
    "# Adding legend\n",
    "plt.legend(['Playoff (0)', 'Playoff (1)'], title='Playoff')\n",
    "\n",
    "# Show the plot\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
   "id": "bbb8de51-dac0-4c33-801a-55075e02f5b6",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "<Figure size 1000x600 with 0 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Sample data\n",
    "data = {\n",
    "    'Team': ['Team A', 'Team B', 'Team C', 'Team A', 'Team B'],\n",
    "    'Season': ['2018', '2019', '2020', '2021', '2022'],\n",
    "    'Salaries': [200, 220, 240, 260, 250],  # Total salaries in millions\n",
    "    'QB_Pay_Percentage': [15, 20, 25, 30, 22]  # QB pay percentage of the total salary\n",
    "}\n",
    "\n",
    "# Create a DataFrame\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Calculate QB salary based on QB pay percentage\n",
    "df['QB_Salary'] = (df['QB_Pay_Percentage'] / 100) * df['Salaries']\n",
    "\n",
    "# Calculate non-QB salary\n",
    "df['Non_QB_Salary'] = df['Salaries'] - df['QB_Salary']\n",
    "\n",
    "# Prepare data for the stacked bar chart\n",
    "stacked_data = df[['Season', 'Non_QB_Salary', 'QB_Salary']]\n",
    "\n",
    "# Create a stacked bar chart\n",
    "plt.figure(figsize=(10, 6))\n",
    "stacked_data.set_index('Season').plot(kind='bar', stacked=True, color=['blue', 'red'], edgecolor='black')\n",
    "\n",
    "# Adding titles and labels\n",
    "plt.title('Team Salaries and Quarterback Pay Percentage')\n",
    "plt.xlabel('Seasons')\n",
    "plt.ylabel('Salaries (in millions)')\n",
    "\n",
    "# Adding legend\n",
    "plt.legend(['Non-QB Salaries', 'QB Salaries'], title='Salary Breakdown')\n",
    "\n",
    "# Show the plot\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
