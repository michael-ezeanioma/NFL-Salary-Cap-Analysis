   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Sample data\n",
    "data = {\n",
    "    'Team': ['Team A', 'Team B', 'Team C', 'Team A', 'Team B'],\n",
    "    'Season': ['2018', '2019', '2020', '2021', '2022'],\n",
    "    'Playoff': [1, 0, 1, 0, 1],\n",
    "    'Salaries': [150, 200, 250, 300, 180]  # Example salaries in millions\n",
    "}\n",
    "\n",
    "# Create a DataFrame\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Pivot the DataFrame to prepare for the stacked bar chart\n",
    "pivot_df = df.pivot_table(index='Season', columns='Playoff', values='Salaries', aggfunc='sum', fill_value=0)\n",
    "\n",
    "# Create a stacked bar chart\n",
    "plt.figure(figsize=(10, 6))\n",
    "pivot_df.plot(kind='bar', stacked=True, color=['red', 'blue'], edgecolor='black')\n",
    "\n",
    "# Adding titles and labels\n",
    "plt.title('Salaries vs. Seasons (Stacked by Playoff Appearance)')\n",
    "plt.xlabel('Seasons')\n",
    "plt.ylabel('Salaries (in millions)')\n",
    "\n",
    "# Adding legend\n",
    "plt.legend(['Playoff (0)', 'Playoff (1)'], title='Playoff')\n",
    "\n",
    "# Show the plot\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
  },
