   "source": [
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Sample data\n",
    "data = {\n",
    "    'Team': ['Team A', 'Team A', 'Team A', 'Team B', 'Team B', 'Team B'],\n",
    "    'Season': ['2018', '2019', '2020', '2018', '2019', '2020'],\n",
    "    'QB_P': [4000, 4500, 4800, 3900, 4300, 4200],  # Example QB passing yards\n",
    "    'Salaries': [150, 180, 200, 160, 190, 210],  # Example salaries in millions\n",
    "    'Playoff': [1, 1, 1, 0, 1, 0]  # Playoff appearances (1 or 0)\n",
    "}\n",
    "\n",
    "# Create a DataFrame\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Pivot the DataFrame for plotting\n",
    "df_pivot = df.pivot(index='Season', columns='Team', values='QB_P')\n",
    "\n",
    "# Create a line chart for QB passing yards\n",
    "plt.figure(figsize=(12, 6))\n",
    "for team in df['Team'].unique():\n",
    "    plt.plot(df_pivot.index, df_pivot[team], marker='o', label=team)\n",
    "\n",
    "# Adding titles and labels\n",
    "plt.title('Quarterback Passing Yards Over Seasons')\n",
    "plt.xlabel('Season (Year)')\n",
    "plt.ylabel('Quarterback Passing Yards (QB_P)')\n",
    "plt.xticks(rotation=45)\n",
    "\n",
    "# Adding a secondary y-axis for Salaries\n",
    "ax2 = plt.gca().twinx()\n",
    "for team in df['Team'].unique():\n",
    "    ax2.plot(df['Season'].unique(), df[df['Team'] == team]['Salaries'], linestyle='--', label=f'{team} Salaries', alpha=0.5)\n",
    "\n",
    "# Adding secondary y-axis label\n",
    "ax2.set_ylabel('Salaries (in millions)')\n",
    "\n",
    "# Adding legends\n",
    "plt.legend(loc='upper left')\n",
    "ax2.legend(loc='upper right')\n",
    "\n",
    "# Show the plot\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
  },
