    "\n",
    "# Sample DataFrame creation\n",
    "data = {\n",
    "    'QB_P': [100, 200, 300, 400, 500, 600, 700],\n",
    "    'Seasons': [2013, 2014, 2016, 2018, 2020, 2021, 2022],\n",
    "    'Playoffs': [0, 0, 1, 1, 0, 1, 1]  # 1 for playoff teams, 0 otherwise\n",
    "    #Teams(Colorcode)\n",
    "}\n",
    "\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Create the scatter plot\n",
    "plt.figure(figsize=(10, 6))\n",
    "scatter = plt.scatter(df['Seasons'], df['QB_P'], c=df['Playoffs'], cmap='coolwarm', s=100)\n",
    "\n",
    "# Add a colorbar\n",
    "cbar = plt.colorbar(scatter)\n",
    "cbar.set_ticks([0, 1])\n",
    "cbar.set_ticklabels(['No Playoffs', 'Playoffs'])\n",
    "\n",
    "# Set labels and title\n",
    "plt.title('Correlation Between Money Spent and Winning Records')\n",
    "plt.xlabel('Season')\n",
    "plt.ylabel('Money Spent (QB_P)')\n",
    "\n",
    "# Show the plot\n",
    "plt.grid()\n",
    "plt.show()\n"
   ]
