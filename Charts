    "import pandas as pd\n",
    "\n",
    "# Sample data\n",
    "data = {\n",
    "    'Team': ['Team A', 'Team B', 'Team C', 'Team A', 'Team B'],\n",
    "    'Season': ['2018', '2019', '2020', '2021', '2022'],\n",
    "    'Salaries': [200, 220, 240, 260, 250],  # Total salaries in millions\n",
    "    'QB_Pay_Percentage': [5, 12, 15, 20, 25]  # QB pay percentage of the total salary\n",
    "}\n",
    "\n",
    "# Create a DataFrame\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Filter for QB pay percentages greater than 10%\n",
    "df_filtered = df[df['QB_Pay_Percentage'] > 10]\n",
    "\n",
    "# Calculate QB salary based on QB pay percentage\n",
    "df_filtered['QB_Salary'] = (df_filtered['QB_Pay_Percentage'] / 100) * df_filtered['Salaries']\n",
    "\n",
    "# Calculate non-QB salary\n",
    "df_filtered['Non_QB_Salary'] = df_filtered['Salaries'] - df_filtered['QB_Salary']\n",
    "\n",
    "# Prepare data for the stacked bar chart\n",
    "stacked_data = df_filtered[['Season', 'Non_QB_Salary', 'QB_Salary']]\n",
    "\n",
    "# Create a stacked bar chart\n",
    "plt.figure(figsize=(10, 6))\n",
    "stacked_data.set_index('Season').plot(kind='bar', stacked=True, color=['green', 'orange'], edgecolor='black')\n",
    "\n",
    "# Adding titles and labels\n",
    "plt.title('Team Salaries with QB Pay Percentage > 10%')\n",
    "plt.xlabel('Seasons')\n",
    "plt.ylabel('Salaries (in millions)')\n",
    "\n",
    "# Adding legend\n",
    "plt.legend(['Non-QB Salaries', 'QB Salaries'], title='Salary Breakdown')\n",
    "\n"
   ]
